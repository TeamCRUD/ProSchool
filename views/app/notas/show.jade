extends ../layout_app

block content_app
    .show-content 
        .show-main
            h1= nota.description + ' : ' + nota.nota
            if(typeof nota.profesor != "undefined")
                p #{nota.periodo + ' periodo'} | Profesor #{nota.profesor.name  + ' ' + nota.profesor.lastname } | Estudiante #{nota.student.name + ' ' + nota.student.lastname}